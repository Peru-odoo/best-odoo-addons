<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="view_res_company_push_notification_form" model="ir.ui.view">
        <field name="name">res.company.push.notification.form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="push_notification" string="Push Notification">
                    <group string="FCM Configuration">
                        <field name="fcm_server_id" />
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
